{
    "require": {
        "php": "8.0.*",
        "symfony/console": "3.4.*",
        "symfony/dotenv": "3.4.*",
        "symfony/flex": "^1.17",
        "symfony/framework-bundle": "3.4.*",
        "symfony/twig-bundle": "3.4.*",
        "symfony/yaml": "3.4.*"
    },
    "require-dev": {
        "rector/rector": "^0.11.59",
        "sensio/generator-bundle": "^3.1"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Org\\": "src/Org"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        }
    }
}
